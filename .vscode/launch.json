{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run WebExample",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build-web-example",
      "program": "${workspaceFolder}/WebExample/bin/Debug/net7.0/WebExample.dll",
      "args": ["--urls", "http://+:5100"],
      "cwd": "${workspaceFolder}/WebExample",
      "stopAtEntry": false,
      "console": "integratedTerminal"
    }
  ],
  "compounds": [
    {
      "name": "Run All",
      "configurations": ["Run WebExample"]
    }
  ]
}
